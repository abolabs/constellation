{
  "name": "abolabs/constellation",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "corbosman/laravel-passport-claims": "^5",
    "guzzlehttp/guzzle": "^7.5",
    "guzzlehttp/psr7": "^2.5",
    "http-interop/http-factory-guzzle": "^1.2",
    "infyomlabs/laravel-generator": "10.x-dev",
    "laravel/framework": "^10.0",
    "laravel/passport": "^11.0",
    "laravel/scout": "^10.1",
    "laravel/tinker": "^2.8",
    "mariuzzo/laravel-js-localization": "^1.8",
    "meilisearch/meilisearch-php": "^1.1.0",
    "owen-it/laravel-auditing": "^13.0",
    "predis/predis": "^2.1",
    "spatie/laravel-permission": "^5.10",
    "swagger-api/swagger-ui": "^4.0",
    "zircote/swagger-php": "2.*"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.12",
    "doctrine/dbal": "^3.6",
    "fakerphp/faker": "^1.21",
    "icanhazstring/composer-unused": "^0.8.7",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^7.0",
    "phpro/grumphp": "^1.16",
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.6",
    "styleci/cli": "^1.2",
    "tanmuhittin/laravel-google-translate": "^2.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "phpro/grumphp": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
